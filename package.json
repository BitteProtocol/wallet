{
  "name": "@bitte-ai/wallet",
  "version": "0.6.6-beta-prerelease.6",
  "description": "Bitte Wallet",
  "keywords": [
    "bitte",
    "wallet"
  ],
  "scripts": {
    "watch": "tsc && tsc --watch & jest --watch --coverage",
    "lint": "eslint . --fix --ext ts --ext tsx",
    "build": "tsup --config tsup.config.ts --external fsevents",
    "prepare": "pnpm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "author": "Bitte Team",
  "license": "MIT",
  "dependencies": {
    "@near-wallet-selector/core": "latest",
    "@near-wallet-selector/wallet-utils": "latest",
    "near-api-js": "latest"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}
